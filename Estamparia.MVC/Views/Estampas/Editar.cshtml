@model Estamparia.MVC.Models.Estampa

@{
    ViewBag.Title = "Editar";
}

@{
    string layoutValue = Request.Cookies.Get("LayoutName").Value;

    switch (layoutValue)
    {
        case "Bootstrap":
            Html.RenderPartial("_EditarBootstrap", Model);
            break;

        case "Ink":
            Html.RenderPartial("_EditarInk", Model);
            break;

        case "Zurb":
            Html.RenderPartial("_EditarZurb", Model);
            break;

    }
}
