@model Estamparia.MVC.Models.Estampa

@{
    ViewBag.Title = "Excluir";
}


@{
    string layoutValue = Request.Cookies.Get("LayoutName").Value;

    switch (layoutValue)
    {
        case "Bootstrap":
            Html.RenderPartial("_ExcluirBootstrap", Model);
            break;

        case "Ink":
            Html.RenderPartial("_ExcluirInk", Model);
            break;

        case "Zurb":
            Html.RenderPartial("_ExcluirZurb", Model);
            break;

    }
}