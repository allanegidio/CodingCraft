@model IEnumerable<Estamparia.MVC.Models.Estampa>

@{
    ViewBag.Title = "Indice";
}


@{
    string layoutValue = Request.Cookies.Get("LayoutName").Value;

    switch (layoutValue)
    {
        case "Bootstrap":
            Html.RenderPartial("_IndiceBootstrap", Model);
            break;

        case "Ink":
            Html.RenderPartial("_IndiceInk", Model);
            break;

        case "Zurb":
            Html.RenderPartial("_IndiceZurb", Model);
            break;

    }
}