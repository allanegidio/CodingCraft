@model Estamparia.MVC.Models.Estampa

@{
    ViewBag.Title = "Criar";
    Layout = "~/Views/Shared/_LayoutBootstrap.cshtml";
}

@{
    string layoutValue = Request.Cookies.Get("LayoutName").Value;

    switch (layoutValue)
    {
        case "Bootstrap":
            Html.RenderPartial("_CriarBootstrap", Model);
            break;

        case "Ink":
            Html.RenderPartial("_CriarInk", Model);
            break;

        case "Zurb":
            Html.RenderPartial("_CriarZurb", Model);
            break;

    }
}



