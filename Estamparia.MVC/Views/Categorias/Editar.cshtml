@model Estamparia.MVC.Models.Categoria

@{
    ViewBag.Title = "Editar";
}

@{
    string layoutValue = Request.Cookies.Get("LayoutName").Value;

    switch (layoutValue)
    {
        case "Bootstrap":
            Html.RenderPartial("Bootstrap/_Editar", Model);
            break;

        case "Ink":
            Html.RenderPartial("Ink/_Editar", Model);
            break;

        case "Zurb":
            Html.RenderPartial("Zurb/_Editar", Model);
            break;

    }
}