@model IEnumerable<Estamparia.MVC.Models.Categoria>

@{
    ViewBag.Title = "Indice";
}

@{
    string layoutValue = Request.Cookies.Get("LayoutName").Value;

    switch (layoutValue)
    {
        case "Bootstrap":
            Html.RenderPartial("Bootstrap/_Indice", Model);
            break;

        case "Ink":
            Html.RenderPartial("Ink/_Indice", Model);
            break;

        case "Zurb":
            Html.RenderPartial("Zurb/_Indice", Model);
            break;

    }
}