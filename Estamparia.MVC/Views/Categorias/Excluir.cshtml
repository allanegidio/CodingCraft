@model Estamparia.MVC.Models.Categoria

@{
    ViewBag.Title = "Excluir";
}

@{
    string layoutValue = Request.Cookies.Get("LayoutName").Value;

    switch (layoutValue)
    {
        case "Bootstrap":
            Html.RenderPartial("Bootstrap/_Excluir", Model);
            break;

        case "Ink":
            Html.RenderPartial("Ink/_Excluir", Model);
            break;

        case "Zurb":
            Html.RenderPartial("Zurb/_Excluir", Model);
            break;

    }
}